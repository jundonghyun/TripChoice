<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.tripchoice">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TripChoice">
        <activity android:name=".CloudFireStoreExample"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SearchActivity"/>
        <activity android:name=".JoinActivity"/>
        <activity android:name=".LoginActivity"/>
        <activity android:name=".NoticeActivity"/>
        <activity android:name=".MapActivity"/>
        <activity android:name=".MyidActivity"/>
        <activity android:name=".RestaurantActivity"/>
        <activity android:name=".ActivityActivity"/>
    </application>

</manifest>